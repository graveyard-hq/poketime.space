{
  "name": "@poketime/backend",
  "version": "1.0.0",
  "main": "./src/main.ts",
  "scripts": {
    "start": "export COMMIT_SHA=$(git rev-parse --short HEAD) NODE_ENV=production && node ./build",
    "dev": "export COMMIT_SHA=$(git rev-parse --short HEAD) NODE_ENV=development && tsx --watch ./src/index.ts | pino-pretty",
    "build": "rm -rf build && pnpm db:generate && tsc",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:generate": "prisma generate",
    "clean": "rm -rf node_modules build",
    "format": "prettier --write **/*"
  },
  "dependencies": {
    "@fastify/auth": "^4.6.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/etag": "^5.1.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@prisma/client": "^5.10.2",
    "argon2": "^0.40.1",
    "axios": "^1.6.7",
    "bullmq": "^5.4.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.1",
    "fastify-raw-body": "^4.3.0",
    "fastify-yup-schema": "^1.0.4",
    "form-data": "^4.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mailgun.js": "^10.2.1",
    "node-cron": "^3.0.3",
    "time2fa": "^1.3.0",
    "validator": "^13.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.24",
    "@types/node-cron": "^3.0.11",
    "@types/validator": "^13.11.9",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}